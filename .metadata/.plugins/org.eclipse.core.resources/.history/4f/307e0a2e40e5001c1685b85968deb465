function checkInterviewForm() {
	let checkResult = true;

	let postDate = new Date($("#intTime").val());
    let nowDate = new Date();
    if (postDate < nowDate) {
		alert("\"面試日期\"不可超過今日");
        checkResult = false;
    }
	return checkResult;
 } 


$('#wrongInput').click(function() {
	$('#compid').val('A7654321')
	$('#compwd').val('abc123zzz')
	$('#corpname').val('咻咻股份有限公司')
